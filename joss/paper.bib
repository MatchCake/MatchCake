@misc{bravyi_contraction_2008,
	title = {Contraction of matchgate tensor networks on non-planar graphs},
	url = {http://arxiv.org/abs/0801.2989},
	abstract = {A tensor network is a product of tensors associated with vertices of some graph \$G\$ such that every edge of \$G\$ represents a summation (contraction) over a matching pair of indexes. It was shown recently by Valiant, Cai, and Choudhary that tensor networks can be efficiently contracted on planar graphs if components of every tensor obey a system of quadratic equations known as matchgate identities. Such tensors are referred to as matchgate tensors. The present paper provides an alternative approach to contraction of matchgate tensor networks that easily extends to non-planar graphs. Specifically, it is shown that a matchgate tensor network on a graph \$G\$ of genus \$g\$ with \$n\$ vertices can be contracted in time \$T=poly(n) + 2{\textasciicircum}\{2g\} O(m{\textasciicircum}3)\$ where \$m\$ is the minimum number of edges one has to remove from \$G\$ in order to make it planar. Our approach makes use of anticommuting (Grassmann) variables and Gaussian integrals.},
	number = {{arXiv}:0801.2989},
	publisher = {{arXiv}},
	author = {Bravyi, Sergey},
	urldate = {2023-10-19},
	date = {2008-01-18},
	eprinttype = {arxiv},
	eprint = {0801.2989 [quant-ph]},
	keywords = {Quantum Physics},
	file = {arXiv.org Snapshot:C\:\\Users\\gince\\Zotero\\storage\\QQWQQN8M\\0801.html:text/html;Full Text PDF:C\:\\Users\\gince\\Zotero\\storage\\JR9RV3WQ\\Bravyi - 2008 - Contraction of matchgate tensor networks on non-pl.pdf:application/pdf},
}

